{
  "manifest_version": 2,

  "name": "WashDog",
  "description": "experimental",
  "version": "0.1",

  "browser_action": {
  	"default_title": "bird up",
    "default_icon": "clean.png",
    "default_popup": "popup.html"
  },

    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["bundle.js"]
    }
  ],

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "downloads"

  ]

}
